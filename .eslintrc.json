{
    "env": {
        "node": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style": "off",
        "no-case-declarations": "off",
        "no-unused-vars": "warn",
        "no-dupe-keys": "warn",
        "no-undef": "warn"
        ,
        // "indent": [
        //     "error",
        //     4
        // ],
        "quotes": [
            "warn",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-var": [
            "error"
        ],
        "max-lines": [
            "error",
            550
        ]
    }
}
